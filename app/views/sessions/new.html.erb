<h1>Welcome to OPEN</h1>
<h2>We care about your privacy</h2>

<%=form_tag users_path do%>
<label>Name:</label><br>
<%=text_field_tag :name %><br>
<label>Password: </label><br>
<%=text_field_tag :password %><br>
 <%= hidden_field_tag :authenticity_token, form_authenticity_token %><br>

 <%= submit_tag "Login" %><br>
<% end %>



 <br><%=link_to "Create Account", new_user_path%>