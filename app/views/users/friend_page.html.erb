 <%= form_with(url: "/users", method: "get") do %>
   <%= label_tag(:q, "Search for users:") %>
   <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
 <% end %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user_path(user) %> 
    </li>
    <br>
  <% end %>
</ul>



<h2>Your have <%= @user.friends.count %> friends:</h2>

<% @user.friends.each do |f| %>
    <p><%= link_to(f.name, user_path(f))%></p>
<% end %>

<p><%= link_to("Back", user_path(@user)) %></p>